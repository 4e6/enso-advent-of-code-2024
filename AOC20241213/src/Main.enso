import local.AOC20241213

## ICON group
parse_button node1 =
    any2 = node1.find 'Button \\w: X[+](\\d+), Y[+](\\d+)'
    vector2 = any2.groups
    node2 = vector2.at 1
    node3 = vector2.at 2
    vector3 = [node2, node3]
    vector3

## ICON group
parse_prize node3 =
    any3 = node3.find 'Prize: X=(\\d+), Y=(\\d+)'
    vector2 = any3.groups
    node4 = vector2.at 1
    node5 = vector2.at 2
    vector4 = [node4, node5]
    vector4

## ICON group
parse_machine any1 =
    node1 = any1.at 0
    node2 = any1.at 1
    any2 = AOC20241213.parse_button node2
    node3 = any1.at 2
    vector4 = Main.parse_prize node3
    vector3 = Main.parse_button node1
    vector1 = [vector3, any2, vector4]
    vector2 = vector1.flatten
    node4 = vector2.map .parse_integer
    node4

parse_input lines =
    go input acc =
        if input.is_empty then acc else
            p = input.take 4
            m = parse_machine p
            IO.println ('parse_machine: ' + input.length.to_text)
            @Tail_Call go (input.drop 4) acc+[m]
    go lines []

## xa + yc = k
   xb + yd = l
   ---
   x = k-yc / a
   xba + yda = la
   ---
   b(k-yc) + yda = la
   ---
   bk - ycb + yda = la
   ---
   y(da-cb) = la-bk
   ---
   y = la-bk / da-cb
   x = k-yc / a
solve machine =
    a = machine.at 0
    b = machine.at 1
    c = machine.at 2
    d = machine.at 3
    k = machine.at 4
    l = machine.at 5
    y = (l*a - b*k) / (d*a - c*b)
    if y.floor != y then [0, 0] else
        x = (k - y*c) / a
        [x, y]

to_tokens result =
    (3 * result.at 0) + result.at 1

compute input =
    input.fold_with_index 0 acc-> ix-> machine->
        result = solve machine
        acc + to_tokens result

main =
    text1 = 'bench.txt'
    quot1 = enso_project.data / text1
    file1 = Data.read quot1
    vector1 = file1.lines
    any1 = vector1.take 4
    vector3 = Main.parse_machine any1
    any2 = AOC20241213.parse_input vector1
    node1 = any2.at 0
    IO.println 'node1'
    any3 = AOC20241213.solve node1
    any4 = AOC20241213.to_tokens any3
    any5 = AOC20241213.compute any2
    any5



